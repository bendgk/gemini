Get the connection rate limits.

Request: {
  "id": "1",
  "method": "conninfo",
  "params": {}
}

Response: {
  "id": "1",
  "status": 200,
  "rateLimits": [
    {
      "rateLimitType": "REQUEST_WEIGHT",
      "interval": "MINUTE",
      "intervalNum": 1,
      "limit": 30000,
      "count": 1
    },
    {
      "rateLimitType": "ORDERS",
      "interval": "SECOND",
      "intervalNum": 10,
      "limit": 1000,
      "count": 0
    },
    {
      "rateLimitType": "ORDERS",
      "interval": "DAY",
      "intervalNum": 1,
      "limit": 1000000,
      "count": 0
    },
    {
      "rateLimitType": "CONNECTION_ATTEMPTS",
      "interval": "MINUTE",
      "intervalNum": 5,
      "limit": 300,
      "count": 1
    }
  ]
}